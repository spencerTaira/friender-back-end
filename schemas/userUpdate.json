{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "hobbies": {
      "type": "array"
    },
    "interests": {
      "type": "array"
    },
    "zip": {
      "type": "string",
      "minLength": 5,
      "maxLength": 10
    }
  },
  "additionalProperties": false,
  "required": [
    "firstName",
    "lastName",
    "hobbies",
    "interests",
    "zip"
  ]
}
